import Layout from '../layouts/Layout.astro';
import translation from '../locales/en.json';
import '../styles/privacy.scss';
const {lang = "en" } = Astro.props;

---
<style lang="scss">
ul, ol, li {
    display: list-item;
}
ul>li{
    list-style:disc;
}
ul,ol{
    padding:0 2rem;
}
.privacy_title {
    font-size: 2rem;
    font-weight: 700;
    line-height: 2.5rem;
    color: #AA823A;
}
.privacy_content{
    font-family: 'Open Sans', sans-serif;
    padding: 5% 10%;
    color: #535353;
    font-weight: 300;
    font-size: 0.9rem;
    line-height: 1.5rem;
    p{
        margin:1rem 0;
    }
    ol{
        margin: 0 0 20px 0;
    }
    td, th {
    border: 1px solid #cdcdcd;
    padding: 8px;
    p{
        margin:0;
    }
	}
}
</style>
<Layout lang={lang} title="Privacy Policy" description="Our privacy policy">

